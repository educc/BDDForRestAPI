{
  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Client Object",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "data": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "address": {
          "type": "null"
        }
      },
      "required": [ "name", "lastName", "address" ]
    }
  },
  "required": [ "code", "data" ]
}
